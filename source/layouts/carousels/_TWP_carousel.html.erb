<div id="twp_carousel_controls" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active" id="carousel">
      <%= image_tag "TWP_landing.png", class: "d-inline-block w-100", id: "desktop-image", data: { toggle: 'modal', target: '#TWP_MODAL' } %>
    </div>
    <div class="carousel-item">
      <%= image_tag "TWP_waiting.png", class: "d-inline-block w-100", data: { toggle: 'modal', target: '#TWP_MODAL' } %>
    </div>
    <div class="carousel-item">
      <%= image_tag "TWP_chat.png.jpg", class: "d-inline-block w-100", data: { toggle: 'modal', target: '#TWP_MODAL' } %>
    </div>
    <div class="carousel-item">
      <%= image_tag "TWP_show.jpg", class: "d-inline-block w-100", data: { toggle: 'modal', target: '#TWP_MODAL' } %>
    </div>
    <div class="carousel-item">
      <%= image_tag "TWP_Ripley.jpg", class: "d-inline-block w-100", data: { toggle: 'modal', target: '#TWP_MODAL' } %>
    </div>
  </div>
  <a class="carousel-control-prev" href="#twp_carousel_controls" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#twp_carousel_controls" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
